# ===================================================================
# ALISTO APP - ENVIRONMENT VARIABLES EXAMPLE
# ===================================================================
# Copy this file to .env and update with your actual values
# ===================================================================

# Clerk Authentication
EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_your_clerk_publishable_key_here

# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:5000/api
EXPO_PUBLIC_API_KEY=your_api_key_here
EXPO_PUBLIC_ENVIRONMENT=development

# Optional: Additional API endpoints
EXPO_PUBLIC_FILE_UPLOAD_URL=http://localhost:5000/api/files
EXPO_PUBLIC_WEBSOCKET_URL=ws://localhost:5000/ws

# App Configuration
EXPO_PUBLIC_APP_NAME=Alisto
EXPO_PUBLIC_APP_VERSION=1.0.0

# Feature Flags
EXPO_PUBLIC_ENABLE_OFFLINE_MODE=true
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=true
EXPO_PUBLIC_ENABLE_ANALYTICS=false

# Cache Configuration
EXPO_PUBLIC_CACHE_DURATION_SHORT=120000
EXPO_PUBLIC_CACHE_DURATION_MEDIUM=300000
EXPO_PUBLIC_CACHE_DURATION_LONG=900000